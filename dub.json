  "name": "dedcpu",
  "authors": ["Luis Panadero GuardeÃ±o"],
  "targetType": "none",
  "license": "BSD 3-clause",
  "description": "DCPU-16 tools",
  "subPackages": [
    {
      "name": "lem1802",
      "description": "Visual LEM1802 font editor",
      "targetType": "executable",
      "targetName": "lem1802",
      "excludedSourceFiles": [
        "src/bconv.d",
        "src/ddis.d"
      ],
      "dependencies": {
        "gtk-d:gtkd": "~>3.2.0"
      }
    },
    {
      "name": "bconv",
      "description": "Binary file conversor. Converts between different data files for DCPU-16 emulators",
      "targetType": "executable",
      "targetName": "bconv",
      "excludedSourceFiles": [
        "src/lem1802_fontview.d",
        "src/ddis.d",
        "src/ui/*"
      ]
    },
    {
      "name": "ddis",
      "description": "Dis-assembler for DCPU-16. Generates a DCPU-16 assembly dump from a binary file.",
      "targetType": "executable",
      "targetName": "ddis",
      "excludedSourceFiles": [
        "src/lem1802_fontview.d",
        "src/bconv.d",
        "src/ui/*"
      ]
    }
  ]
}
